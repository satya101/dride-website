{
	"name": "dride-website",
	"version": "0.0.0",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/dride/dride-website.git"
	},
	"scripts": {
		"ng": "ng",
		"start": "ng serve",
		"build": "ng build",
		"build:client-and-server-bundles": "ng build --prod && ng run dride-website:server:production ",
		"build:prerender": "npm run build:client-and-server-bundles && npm run webpack:server && npm run generate:prerender",
		"generate:static": "cd functions/dist && node prerender",
		"webpack:server": "webpack --config webpack.server.config.js --progress --colors",
		"generate:prerender": "cd dist && node prerender",
		"serve:ssr": "node dist/server",
		"build:ssr": "npm run build:client-and-server-bundles && npm run webpack:server",
		"release": "ng build --prod && node scripts/deploySequence.js && firebase deploy --only hosting,functions:metaService && sentry-cli releases -o dride-inc -p Website files 2.0 upload-sourcemaps --url-prefix / dist",
		"release:ssr": "npm run build:dynamic && npm run serve:dynamic && node scripts/deploySequenceSSR.js",
		"test": "ng test",
		"lint": "ng lint",
		"e2e": "ng e2e",
		"bundle-report": "webpack-bundle-analyzer dist/browser/stats.json"
	},
	"private": true,
